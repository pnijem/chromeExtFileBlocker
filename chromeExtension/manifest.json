{"name": "File Download Controller",
 "description": "A Chrome extension for monitoring and allowing/blocking file downloads",
 "version": "1.0",
 "background": {
   "scripts": ["jquery-3.3.1.min.js","bgEvent.js"],
   "persistent": false
 },
 "options_page": "options.html",
 "permissions": ["downloads", "https://e60eb57tz8.execute-api.us-east-2.amazonaws.com/beta/fileblocker/*"],
 "content_security_policy": "script-src 'self'; object-src 'self'",
 "manifest_version": 2}
